<p id="notice"><%= notice %></p>

<h1>Order Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Order Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orderstatuses.each do |orderstatus| %>
      <tr>
        <td><%= orderstatus.orderstatus %></td>
        <td><%= link_to 'Show', orderstatus %></td>
        <td><%= link_to 'Edit', edit_orderstatus_path(orderstatus) %></td>
        <td><%= link_to 'Destroy', orderstatus, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Status', new_orderstatus_path %>
