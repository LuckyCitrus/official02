<p id="notice"><%= notice %></p>

<h1>Dummyroles</h1>

<table>
  <thead>
    <tr>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dummyroles.each do |dummyrole| %>
      <tr>
        <td><%= dummyrole.role %></td>
        <td><%= link_to 'Show', dummyrole %></td>
        <td><%= link_to 'Edit', edit_dummyrole_path(dummyrole) %></td>
        <td><%= link_to 'Destroy', dummyrole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dummyrole', new_dummyrole_path %>
