<div class="row row-fluid">
    <div class="col-sm-5">
        <div class="card">
            <div class="card-body">
                <p>
                    <strong>Warehouse Name:</strong>
                    <%= @warehouse.warehousename %>
                </p>
                <p>
                    <strong>Location:</strong>
                    <%= @warehouse.location.city %>
                </p>

                <%= link_to 'Edit', edit_warehouse_path(@warehouse), class: "btn btn-primary" %>
                <%= link_to 'Back', warehouses_path, class: "btn btn-secondary" %>
            </div>
        </div>
    </div>
</div>