<%= form_with(model: paymentstatus, local: true) do |form| %>
<% if paymentstatus.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(paymentstatus.errors.count, "error") %> prohibited this paymentstatus from being saved:</h2>

    <ul>
        <% paymentstatus.errors.full_messages.each do |message| %>
        <li><%= message %></li>
