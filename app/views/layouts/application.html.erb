<!DOCTYPE html>
<html>

<head>
    <title>MitsabWebApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag asset_path('image-name.png') %>
</head>

<body>
    <div id="header">
        <h1>MitsabWebApp</h1>
        <%= link_to "Employees", employees_path %> | <%= link_to "Customers", customers_path %> | <%= link_to "Cars", cars_path %> | <%= link_to "Orders", orders_path %> | <%= link_to "Invoices", invoices_path %> | <%= link_to "Payments", payments_path %> | <%= link_to "Shipments", shipments_path %>
    </div>
    <%= yield %>
</body>

</html>